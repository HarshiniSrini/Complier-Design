# include <stdio.h> 
# include <conio.h> 
# include<string.h>
struct op {
char left[10];
char right[10];
}
op2[15];
int main()
{
int i,j,n;
char *tem,*mat;
clrscr();
printf("enter no.of.values");
scanf("%d",&n);
for(i=0;i<n;i++)
{ printf("left");
scanf("%s",op2[i].left);
printf("right");
scanf("%s",op2[i].right);}

printf("dataflow analysis");
for(i=0;i<n;i++)
{
for(j=0;j<n;j++)
{
mat=strstr(op2[j].right,op2[i].left);if (mat)
{
printf("\n%s is live at %s\n",op2[i].left,op2[j].right);
}
}
}
getch();
return 0;
}
